<div class="row-fluid">
	<div class="span2">
		<a href="{{- url }}" target="_blank">
			<img src="{{- img }}" style="padding:5px 0 5px 0"/>
		</a>
	</div>
	<div class="span10">
		<p>
			<span class="lead">{{- track}} &#151; {{- artist}}</span>&nbsp;<small>(<a href="http://youtube.com/watch?v={{- youtube}}" target="_blank"><span class="glyphicon glyphicon-play-circle"></span>youtube</a>)</small><br/>
			<small id="utc-time">{{- date}}</small>
		</p>
	</div>
</div>
